{
  "name": "el-bot",
  "version": "0.9.0-beta.19",
  "description": "A quick qq bot framework for mirai.",
  "author": {
    "name": "云游君",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "homepage": "https://docs.bot.elpsy.cn/",
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/el-bot"
  },
  "bugs": {
    "url": "https://github.com/YunYouJun/el-bot/issues"
  },
  "keywords": [
    "el-bot",
    "elpsy",
    "bot",
    "mirai",
    "qq",
    "framework"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugins/*": {
      "import": "./dist/plugins/*/index.mjs",
      "require": "./dist/plugins/*/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "mcl": {
    "folder": "mcl"
  },
  "scripts": {
    "build": "tsup && npm run copy:pkg",
    "build:api": "npx typedoc",
    "copy:pkg": "esmo scripts/copy.ts",
    "dev": "tsc -w --incremental",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@koa/cors": "^3.4.3",
    "@octokit/webhooks": "^9.26.3",
    "commander": "^9.5.0",
    "consola": "^3.2.3",
    "dayjs": "^1.11.12",
    "download": "^8.0.0",
    "glob": "^11.0.0",
    "html-to-text": "^8.2.1",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "mirai-ts": "^2.4.6",
    "mongoose": "^6.13.0",
    "node-schedule": "^2.1.1",
    "progress": "^2.0.3",
    "qrcode": "^1.5.0",
    "rss-parser": "^3.13.0",
    "shelljs": "^0.8.5",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/download": "^8.0.5",
    "@types/html-to-text": "^8.1.1",
    "@types/inquirer": "^8.2.10",
    "@types/js-yaml": "^4.0.9",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^3.3.1",
    "@types/node": "^17.0.45",
    "@types/node-schedule": "^2.1.7",
    "@types/progress": "^2.0.7",
    "@types/qrcode": "^1.5.5",
    "@types/shelljs": "^0.8.15",
    "@types/xml2js": "^0.4.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "tsup": "^5.12.9",
    "typedoc": "^0.22.18",
    "typescript": "^4.9.5"
  },
  "directories": {
    "lib": "https://github.com/YunYouJun/mirai-ts"
  },
  "ecosystem": {
    "el-bot-api": "https://github.com/ElpsyCN/el-bot-api",
    "el-bot-plugins": "https://github.com/ElpsyCN/el-bot-plugins",
    "el-bot-docs": "https://github.com/ElpsyCN/el-bot-docs",
    "el-bot-template": "https://github.com/ElpsyCN/el-bot-template",
    "el-bot-web": "https://github.com/ElpsyCN/el-bot-web"
  }
}
